{
  "name": "@blokdots/components",
  "version": "2.0.0",
  "description": "blokdots Virtual Components to use in your projects.",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild ./src/index.js --platform=node --loader:.ico=dataurl --loader:.html=dataurl --loader:.svg=dataurl --loader:.css=dataurl --loader:.min.js=dataurl --bundle --outfile=dist/index.js",
    "dev": "esbuild ./src/index.js --platform=node --loader:.ico=dataurl --loader:.html=dataurl --loader:.svg=dataurl --loader:.css=dataurl --loader:.min.js=dataurl --bundle --watch --outfile=dist/index.js",
    "prettier": "prettier --write 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blokdots/components.git"
  },
  "keywords": [
    "blokdots",
    "Arduino",
    "hardware prototyping"
  ],
  "author": "blokdots",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blokdots/components/issues"
  },
  "homepage": "https://github.com/blokdots/components#readme",
  "devDependencies": {
    "esbuild": "^0.12.15",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "johnny-five": "^2.1.0",
    "socket.io": "^4.4.1",
    "ws": "^7.5.3"
  }
}
