{
  "name": "@blokdots/components",
  "version": "2.5.0",
  "description": "blokdots Virtual Components to use in your projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "postinstall": "yarn fix-j5-types",
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup -c -w --bundleConfigAsCjs",
    "prettier": "prettier --write 'src/**/*.{t,j}s'",
    "fix-j5-types": "cd node_modules/@types/johnny-five && curl -LJO https://raw.githubusercontent.com/blokdots/DefinitelyTyped/master/types/johnny-five/index.d.ts && cd ../../../"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blokdots/components.git"
  },
  "keywords": [
    "blokdots",
    "Arduino",
    "hardware prototyping"
  ],
  "author": "blokdots",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blokdots/components/issues"
  },
  "homepage": "https://github.com/blokdots/components#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.4",
    "@rollup/plugin-url": "^8.0.1",
    "@types/qr-image": "^3.2.7",
    "@types/ws": "^8.5.5",
    "johnny-five": "git://github.com/blokdots/johnny-five.git#serialport10",
    "prettier": "^3.0.3",
    "rollup": "^3.29.2",
    "rollup-plugin-base64": "^1.0.1",
    "rollup-plugin-dts": "^6.0.2",
    "socket.io": "^4.7.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "ws": "^8.14.1"
  },
  "dependencies": {
    "node-pixel": "^0.11.0",
    "oled-font-5x7": "^1.0.3",
    "oled-js": "git://github.com/blokdots/oled-js#blokdots",
    "qr-image": "^3.2.0"
  },
  "peerDependencies": {
    "johnny-five": "git://github.com/blokdots/johnny-five.git#serialport10",
    "socket.io": "^4.7.1",
    "ws": "^8.13.0"
  }
}
